(require 'cl)
cl

(setq hoge '(a b c d e f))
(a b c d e f)

(a b c d e f)
hoge
(a b c d e f)

(car hoge)
a


(setf (car hoge) 'A)
A

hoge
(A b c d e f)

(append hoge (list 'g))
(A b c d e f g)

hoge
(A b c d e f)

(cdr hoge)
(b c d e f)
hoge
(A b c d e f)
hoge
(A b c d e f)
